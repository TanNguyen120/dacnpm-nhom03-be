{
  "name": "node-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha test --reporter mochawesome --reporter-options autoOpen=true --experimental-modules --es-module-specifier-resolution=node --timeout 10000 && exit 1",
    "clean": "rmdir /s /q build && mkdir build",
    "build-babel": "babel ./src ./base ./config ./helper ./public ./utils -d ./build -s",
    "builds": "npm run clean && npm run build-babel",
    "build": "node_modules/@babel/cli/bin/babel.js src --source-maps --out-dir dist",
    "production": "node ./build/server.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/server.js",
    "dev": "nodemon --experimental-modules --es-module-specifier-resolution=node src/server.js",
    "devs": "nodemon --exec ./node_modules/.bin/babel-node src/server.js",
    "start-prod": "set NODE_ENV=production && node ./src/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@hapi/joi": "^15.0.3",
    "auto-bind": "^5.0.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.29.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mongoose": "^6.3.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3-rc.0",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^2.0.0",
